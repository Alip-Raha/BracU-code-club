import java.util.Scanner;
public class prog1{
  public static void main(String [] args){
    Scanner sc=new Scanner(System.in);
    System.out.println("Input String");
    String s=sc.nextLine();
    Boolean isPal=true;
    for(int i=0,j=s.length()-1; i<j; i++,j--){
      char ch1=s.charAt(i);
      char ch2=s.charAt(j);
      if(ch1!=ch2){
        isPal=false;
        break;
      }
    }
    System.out.println(isPal);
  }
}
